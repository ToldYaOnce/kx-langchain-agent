{
  "name": "@toldyaonce/kx-langchain-agent-cli",
  "version": "1.0.17",
  "description": "CLI for local development and testing of KxGen LangChain agent",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "kxagent": "./bin/kxagent.js"
  },
  "scripts": {
    "build": "tsc && chmod +x bin/kxagent.js",
    "prepublishOnly": "npm run clean && npm run build",
    "dev": "tsc --watch",
    "clean": "rm -rf lib",
    "test": "jest",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@toldyaonce/kx-langchain-agent-runtime": "^1.0.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "dotenv": "^16.3.0",
    "inquirer": "^9.2.0",
    "ora": "^7.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "9.0.9",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "lib/**/*",
    "src/**/*",
    "bin/**/*",
    "README.md"
  ]
}
