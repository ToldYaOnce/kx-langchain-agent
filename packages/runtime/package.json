{
  "name": "@toldyaonce/langchain-agent-runtime",
  "version": "1.0.13",
  "description": "Lambda handlers and shared libraries for KxGen LangChain agent",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./handlers/agent": {
      "import": "./dist/handlers/agent.js",
      "types": "./dist/handlers/agent.d.ts"
    },
    "./handlers/router": {
      "import": "./dist/handlers/router.js",
      "types": "./dist/handlers/router.d.ts"
    },
    "./handlers/indexer": {
      "import": "./dist/handlers/indexer.js",
      "types": "./dist/handlers/indexer.d.ts"
    },
    "./config/personas": {
      "import": "./dist/config/personas.js",
      "types": "./dist/config/personas.d.ts"
    },
    "./lib/agent": {
      "import": "./dist/lib/agent.js",
      "types": "./dist/lib/agent.d.ts"
    },
    "./lib/config": {
      "import": "./dist/lib/config.js",
      "types": "./dist/lib/config.d.ts"
    },
    "./types/index": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "add:utils": "npm rm @toldyaonce/kx-cdk-lambda-utils && npm i @toldyaonce/kx-cdk-lambda-utils@latest",
    "build": "tsc --project ./tsconfig.json",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.896.0",
    "@aws-sdk/client-eventbridge": "3.896.0",
    "@aws-sdk/lib-dynamodb": "3.896.0",
    "@langchain/aws": "^0.1.0",
    "@langchain/core": "^0.3.0",
    "@langchain/openai": "^0.3.0",
    "@toldyaonce/kx-cdk-lambda-utils": "1.33.0",
    "langchain": "^0.3.0",
    "ulid": "2.4.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.152",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
