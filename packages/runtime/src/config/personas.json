    "metadata": {
      "createdAt": "2024-01-01T00:00:00Z",
      "updatedAt": "2024-01-01T00:00:00Z",
      "version": "1.0.0",
      "tags": ["casual", "friendly", "relaxed", "supportive"]
    },
    "companyInfo": {
      "name": "{{companyName}}",
      "industry": "{{companyIndustry}}",
      "description": "{{companyDescription}}",
      "products": "{{companyProducts}}",
      "benefits": "{{companyBenefits}}",
      "targetCustomers": "{{companyTargetCustomers}}",
      "differentiators": "{{companyDifferentiators}}"
    },
    "goalConfiguration": {
      "enabled": true,
      "goals": [
        {
          "id": "collect_name_first",
          "name": "Collect Name Immediately",
          "description": "Get the user's name right away for personalization",
          "type": "collect_info",
          "priority": "critical",
          "target": {
            "field": "firstName",
            "validation": {
              "required": true,
              "format": "name"
            }
          },
          "timing": {
            "strategy": "immediate",
            "minMessages": 1,
            "maxMessages": 2,
            "triggers": [
              {
                "conditions": [
                  {"type": "message_count", "operator": "greater_than", "value": 0}
                ],
                "logic": "AND"
              }
            ],
            "cooldown": 5
          },
          "approach": {
            "directness": "moderate",
            "contextual": true,
            "valueProposition": "I'd love to personalize this for you",
            "fallbackStrategies": ["tie_to_conversation"]
          },
          "completion": {
            "markComplete": true,
            "response": "Great to meet you!"
          },
          "tracking": {
            "attempts": 0,
            "completed": false,
            "declinedCount": 0
          }
        },
        {
          "id": "collect_email",
          "name": "Collect Email Address",
          "description": "Get the user's email address for follow-up and lead generation",
          "type": "collect_info",
          "priority": "critical",
          "target": {
            "field": "email",
            "validation": {
              "pattern": "^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$",
              "required": true,
              "format": "email"
            }
          },
          "timing": {
            "strategy": "conditional",
            "minMessages": 1,
            "maxMessages": 3,
            "triggers": [
              {
                "conditions": [
                  {"type": "interest_level", "operator": "greater_than", "value": "medium"},
                  {"type": "message_count", "operator": "greater_than", "value": 1}
                ],
                "logic": "AND"
              }
            ],
            "cooldown": 10
          },
          "approach": {
            "directness": "moderate",
            "contextual": true,
            "valueProposition": "I'd love to send you our class schedules and special offers",
            "fallbackStrategies": ["tie_to_membership", "tie_to_scheduling"]
          },
          "completion": {
            "markComplete": true,
            "nextGoals": ["collect_phone"],
            "response": "Perfect! I've got your email saved."
          },
          "tracking": {
            "attempts": 0,
            "completed": false,
            "declinedCount": 0
          }
        },
        {
          "id": "collect_phone",
          "name": "Collect Phone Number",
          "description": "Get the user's phone number for SMS updates and scheduling",
          "type": "collect_info",
          "priority": "critical",
          "target": {
            "field": "phone",
            "validation": {
              "pattern": "^[\\+]?[1-9]?[0-9]{7,15}$",
              "required": true,
              "format": "phone"
            }
          },
          "timing": {
            "strategy": "conditional",
            "minMessages": 3,
            "maxMessages": 6,
            "triggers": [
              {
                "conditions": [
                  {"type": "has_info", "operator": "equals", "value": "email"},
                  {"type": "interest_level", "operator": "greater_than", "value": "low"}
                ],
                "logic": "AND"
              }
            ],
            "cooldown": 15
          },
          "approach": {
            "directness": "moderate",
            "contextual": true,
            "valueProposition": "I can text you class reminders and schedule updates",
            "fallbackStrategies": ["tie_to_convenience", "tie_to_scheduling"]
          },
          "completion": {
            "markComplete": true,
            "nextGoals": ["collect_name"],
            "response": "Great! I'll be able to send you quick updates via text."
          },
          "dependencies": {
            "requires": ["collect_email"]
          },
          "tracking": {
            "attempts": 0,
            "completed": false,
            "declinedCount": 0
          }
        },
        {
          "id": "collect_name",
          "name": "Collect First and Last Name",
          "description": "Get the user's full name for personalization and lead completion",
          "type": "collect_info",
          "priority": "high",
          "target": {
            "field": "fullName",
            "validation": {
              "required": true,
              "format": "name"
            }
          },
          "timing": {
            "strategy": "conditional",
            "minMessages": 4,
            "maxMessages": 7,
            "triggers": [
              {
                "conditions": [
                  {"type": "has_info", "operator": "equals", "value": "email"},
                  {"type": "has_info", "operator": "equals", "value": "phone"}
                ],
                "logic": "AND"
              }
            ],
            "cooldown": 20
          },
          "approach": {
            "directness": "subtle",
            "contextual": true,
            "valueProposition": "I'd love to personalize your experience",
            "fallbackStrategies": ["tie_to_membership_card", "tie_to_class_booking"]
          },
          "completion": {
            "markComplete": true,
            "triggerIntent": "lead_generated",
            "response": "Nice to meet you! I've got all your info saved."
          },
          "dependencies": {
            "requires": ["collect_email", "collect_phone"]
          },
          "tracking": {
            "attempts": 0,
            "completed": false,
            "declinedCount": 0
          }
        },
        {
          "id": "schedule_class",
          "name": "Schedule Future Class",
          "description": "Get the user scheduled for an upcoming fitness class",
          "type": "schedule_appointment",
          "priority": "medium",
          "target": {
            "field": "scheduledClass",
            "validation": {
              "required": false,
              "format": "date"
            }
          },
          "timing": {
            "strategy": "conditional",
            "minMessages": 3,
            "maxMessages": 8,
            "triggers": [
              {
                "conditions": [
                  {"type": "interest_level", "operator": "greater_than", "value": "medium"},
                  {"type": "intent_detected", "operator": "contains", "value": "membership"}
                ],
                "logic": "OR"
              }
            ],
            "cooldown": 30
          },
          "approach": {
            "directness": "moderate",
            "contextual": true,
            "valueProposition": "Let's get you started with a great class experience",
            "fallbackStrategies": ["suggest_trial_class", "offer_gym_tour"]
          },
          "completion": {
            "markComplete": true,
            "response": "Awesome! You're all set for class. I'll send you a reminder."
          },
          "dependencies": {
            "requires": ["collect_email"]
          },
          "tracking": {
            "attempts": 0,
            "completed": false,
            "declinedCount": 0
          }
        }
      ],
      "globalSettings": {
        "maxActiveGoals": 2,
        "respectDeclines": true,
        "adaptToUrgency": true,
        "interestThreshold": 0.3
      },
      "completionTriggers": {
        "allCriticalComplete": "lead_generated",
        "customCombinations": [
          {
            "goalIds": ["collect_email", "collect_phone", "collect_name"],
            "triggerIntent": "lead_generated",
            "description": "Complete lead with all contact information"
          },
          {
            "goalIds": ["collect_email", "collect_phone", "collect_name", "schedule_class"],
            "triggerIntent": "qualified_lead_with_appointment",
            "description": "Fully qualified lead with scheduled appointment"
          }
        ]
      }
    }
  }
